{
  "env": {
    "production": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "browsers": [
                "last 2 version",
                "> 1%"
              ]
            },
            "modules": false,
            "debug": true
          }
        ],
        "stage-2"
      ],
      "plugins": [
        "transform-runtime",
        "transform-vue-jsx",
        "lodash",
        [
          "component",
          {
            "libraryName": "element-ui",
            "styleLibraryName": "~src/assets/el-theme"
          }
        ]
      ]
    },
    "development": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "browsers": [
                "last 2 version",
                "> 1%"
              ]
            },
            "modules": false,
            "debug": true
          }
        ],
        "stage-2"
      ],
      "plugins": [
        "transform-runtime",
        "transform-vue-jsx",
        "lodash",
        [
          "component",
          {
            "libraryName": "element-ui",
            "styleLibraryName": "~src/assets/el-theme"
          }
        ]
      ]
    }
  }
}
